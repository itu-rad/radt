{
    "name": "pg-static-server",
    "version": "0.1.0",
    "description": "Simple TypeScript server that runs a static query against Postgres and returns results",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "pg": "^8.11.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    }
}
